{
  "name": "mxs-parser-antlr",
  "version": "1.0.0",
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4"
  },
  "devDependencies": {
    "@types/antlr4": "^4.11.6",
    "antlr4": "^4.13.1",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "scripts": {
    "dev": "nodemon index.ts",
    "antlr4ts-parserTest": "antlr4ts ./grammar/TypeScriptParser.g4 -o ./build",
    "antlr4ts-lexerTest": "antlr4ts ./grammar/TypeScriptLexer.g4 -o ./build",
    "antlr4ts-lexer": "antlr4ts -visitor ./grammar/mxsLexer.g4 -o ./build",
    "antlr4ts-parser": "antlr4ts -visitor ./grammar/mxsParser.g4 -o ./build",
    "antlr4-lexer": "antlr4 -Dlanguage=TypeScript -visitor -lib ./grammar/mxsLexer.g4 -o ./build/",
    "antlr4-parser": "antlr4 -Dlanguage=TypeScript -visitor -lib ./grammar/mxsParser.g4 -o ./build/"
  }
}
